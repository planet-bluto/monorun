Date="*res://scripts/Date.gd"
WsClient="res://scripts/ws_client.gd"
MultiClient="res://scripts/multiplayer.gd"
MonoBase="*res://scripts/monoBase.gd"
GlobalInputManager="*res://scripts/GlobalInputManager.gd"

[display]

window/size/width=256
window/size/height=144
window/size/resizable=false
window/size/test_width=768
window/size/test_height=432
mouse_cursor/custom_image="res://sprites/invis.png"
window/stretch/mode="viewport"
window/stretch/aspect="expand"


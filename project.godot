config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "DashSecondary",
"language": "GDScript",
"path": "res://scripts/s_dash.gd"
}, {
"base": "Node",
"class": "InputManager",
"language": "GDScript",
"path": "res://scripts/class_input_manager.gd"
} ]
_global_script_class_icons={
"DashSecondary": "",
"InputManager": ""
}

[application]

config/name="Monorun"
run/main_scene="res://scenes/title.tscn"
config/icon="res://icon.png"

[autoload]

GlobalVars="*res://scripts/global_vars.gd"
Network="*res://scripts/network.gd"
